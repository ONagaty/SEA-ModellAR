<plugin>

   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="eu.cessar.ct.newWizards.category"
            name="%cessar.ct.category">
      </category>
      <wizard
            category="eu.cessar.ct.newWizards.category"
            class="eu.cessar.ct.ecuc.workspace.ui.internal.wizards.ModuleDefinitionWizard"
            hasPages="true"
            icon="icons/bsw_md.gif"
            id="eu.cessar.ct.ecuc.workspace.ui.wizards.module.definition"
            name="%wizard.module.definition"
            project="false">
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
         <description>
            %wizards.module.definition.description
         </description>
      </wizard>
      <wizard
            category="eu.cessar.ct.newWizards.category"
            class="eu.cessar.ct.ecuc.workspace.ui.internal.wizards.ModuleConfigurationWizard"
            hasPages="true"
            icon="icons/ecuc_file.gif"
            id="eu.cessar.ct.ecuc.workspace.ui.wizards.module.configuration"
            name="%wizard.module.configuration"
            project="false">
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
         <description>
            %wizards.module.configuration.description
         </description>
      </wizard>
      <wizard
            category="eu.cessar.ct.newWizards.category"
            class="eu.cessar.ct.ecuc.workspace.ui.internal.wizards.NewPlugetWizard"
            hasPages="true"
            icon="icons/pluget.png"
            id="eu.cessar.ct.ecuc.workspace.ui.wizards.newpluget.description"
            name="%wizards.newpluget"
            project="false">
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
         <description>
            %wizards.newpluget.description
         </description>
      </wizard>
   </extension>
